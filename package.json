{
  "name": "@warhol/config-file",
  "version": "1.4.0",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "author": "peter@peterkroener.de",
  "license": "MIT",
  "scripts": {
    "lint": "tslint -p .",
    "test": "ts-node node_modules/.bin/jasmine 'test/*.test.ts'",
    "test:dev": "nodemon --watch src --watch test -e ts,json --exec 'yarn test'",
    "build": "rm -rf dist/ && tsc -d --declarationDir dist/types -m esnext --outDir dist/esm && tsc -m commonjs --outDir dist/cjs",
    "prepublishOnly": "rm -rf node_modules && yarn install && yarn test && yarn build"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jasmine": "^2.8.9",
    "@warhol/tsconfig": "^1.0.0",
    "@warhol/utilities": "^2.0.0",
    "ajv": "^6.5.4",
    "better-ajv-errors": "^0.6.1",
    "jasmine": "^3.3.0",
    "nodemon": "^1.18.5",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.4"
  }
}
